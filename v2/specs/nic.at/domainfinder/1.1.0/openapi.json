    ],
    "x-origin": [
      {
        "format": "openapi",
        "url": "https://domainfinder.nic.at/openapi/",
        "version": "3.0"
      }
    ],
    "x-providerName": "nic.at",
    "x-serviceName": "domainfinder",
    "x-logo": {
      "url": "https://api.apis.guru/v2/cache/logo/https_apis.guru_assets_images_no-logo.svg"
    }
