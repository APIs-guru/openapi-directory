              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotAuthorizedException"
                }
              }
            }
          }
        },
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "ChannelARN",
                  "SenderClientId",
                  "MessagePayload"
                ],
                "properties": {
                  "ChannelARN": {
                    "description": "The ARN of the signaling channel by which Alexa and the master peer communicate.",
                    "type": "string",
                    "pattern": "arn:aws:kinesisvideo:[a-z0-9-]+:[0-9]+:[a-z]+/[a-zA-Z0-9_.-]+/[0-9]+",
                    "minLength": 1,
                    "maxLength": 1024
                  },
                  "SenderClientId": {
                    "description": "The unique identifier for the sender client.",
                    "type": "string",
                    "pattern": "[a-zA-Z0-9_.-]+",
                    "minLength": 1,
                    "maxLength": 256
                  },
                  "MessagePayload": {
                    "description": "The base64-encoded SDP offer content.",
                    "type": "string",
                    "pattern": "[a-zA-Z0-9+/=]+",
                    "minLength": 1,
                    "maxLength": 10000
                  }
                }
              }
            }
          }
        }
      },
      "parameters": [
