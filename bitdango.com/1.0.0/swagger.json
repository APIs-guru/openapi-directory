            "in": "header",
            "name": "Authorization",
            "required": true,
            "type": "string"
          },
          {
            "default": 1,
            "description": "<ul>\r\n</ul>\r\n",
            "in": "query",
            "name": "page",
            "required": false,
            "type": "integer"
          },
